{
  "name": "videomark.webdino.org",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "cms": "netlify-cms-proxy-server",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "format": "prettier --plugin-search-dir . --write .",
    "lint": "npm-run-all lint:*",
    "lint:cspell": "cspell --gitignore '**/*.{css,scss,svelte,js,json,html,yaml}' --dot --no-progress",
    "lint:prettier": "prettier --check --plugin-search-dir=. .",
    "lint:eslint": "eslint --ignore-path .gitignore .",
    "lint:stylelint": "stylelint '**/*.{css,scss,svelte}'",
    "build": "vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@sveltejs/adapter-static": "1.0.0-next.48",
    "@sveltejs/kit": "next",
    "@types/d3-array": "^3.0.3",
    "@types/d3-axis": "^3.0.1",
    "@types/d3-geo": "^3.0.2",
    "@types/d3-scale": "^4.0.2",
    "@types/d3-selection": "^3.0.3",
    "@types/marked": "^4.0.7",
    "@types/topojson": "^3.2.3",
    "cspell": "^6.14.2",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "marked": "^4.2.2",
    "netlify-cms-proxy-server": "^1.3.24",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.8.0",
    "sass": "^1.56.1",
    "stylelint": "^14.14.1",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-scss": "^4.3.0",
    "svelte": "^3.53.1",
    "svelte-check": "^2.9.2",
    "svelte-i18n": "^3.4.0",
    "svelte-preprocess": "^4.10.7",
    "vite": "^3.2.3",
    "yaml": "^2.1.3"
  },
  "type": "module",
  "dependencies": {
    "@mongodb-js/charts-embed-dom": "^2.3.0",
    "d3-array": "^3.2.0",
    "d3-axis": "^3.0.0",
    "d3-geo": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "topojson": "^3.0.2"
  }
}
