{
  "name": "qoe-log-view",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@material-ui/core": "^4.9.4",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.0",
    "@nivo/bar": "^0.61.1",
    "@nivo/calendar": "^0.61.1",
    "@nivo/scatterplot": "^0.61.1",
    "@svgr/webpack": "^5.2.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.0.0",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.1.1",
    "bfj": "^7.0.2",
    "chalk": "^4.0.0",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "dataframe-js": "^1.4.3",
    "date-fns": "^2.9.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^6.0.0",
    "fs-extra": "^9.0.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "jest": "^26.0.0",
    "jest-pnp-resolver": "^1.2.1",
    "jest-resolve": "^26.0.0",
    "material-table": "^1.57.2",
    "material-ui-flat-pagination": "^4.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "p-iteration": "^1.1.8",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dev-utils": "^10.2.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^3.0.0",
    "url-loader": "^4.0.0",
    "uuid": "^8.0.0",
    "videomark-mini-stats": "file:packages/videomark-mini-stats",
    "webpack": "^4.41.6",
    "webpack-dev-server": "^3.10.3",
    "webpack-subresource-integrity": "^1.4.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "npm run build-extension",
    "build-extension": "cross-env PUBLIC_URL=/qoelog node scripts/build.js",
    "build-android": "node scripts/build.js && rimraf build/favicon.png build/unsupported.html build/static/js/*.LICENSE",
    "test": "node scripts/test.js",
    "lint": "eslint --ext .js,.jsx src",
    "lint-report": "eslint --output-file eslint_report.json --format json --ext .js,.jsx src",
    "format": "prettier --write src config scripts"
  },
  "browserslist": [
    "last 1 chrome version"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "workspaces": [
    "packages/*"
  ]
}
