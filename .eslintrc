{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "jsx": true
  },
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "plugins": ["react", "jsx-a11y", "import"],
  "rules": {
    "arrow-body-style": 0,
    "no-console": [
      "error",
      {
        "allow": ["error"]
      }
    ]
  },
  "globals": {
    "chrome": true
  }
}
